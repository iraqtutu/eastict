<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample.mapper.TaskMapper">
  <resultMap id="BaseResultMap" type="eastict.sample.pojo.Task">
    <id column="PROC_NAME_" jdbcType="VARCHAR" property="procName" />
    <id column="STEP_NAME_" jdbcType="VARCHAR" property="stepName" />
    <id column="CUSTOM_CODE_" jdbcType="VARCHAR" property="customCode" />
    <id column="TASK_ID_" jdbcType="VARCHAR" property="taskId" />
    <result column="EXECUTE_ID_" jdbcType="VARCHAR" property="executeId" />
    <result column="BIZ_ID_" jdbcType="VARCHAR" property="bizId" />
    <result column="CREATE_TIME_" jdbcType="TIMESTAMP" property="createTime" />
    <result column="LOCK_FLAG_" jdbcType="CHAR" property="lockFlag" />
    <result column="LOCKED_TIME_" jdbcType="TIMESTAMP" property="lockedTime" />
    <result column="ENDLOCK_TIME_" jdbcType="TIMESTAMP" property="endlockTime" />
    <result column="HUNG_FLAG_" jdbcType="CHAR" property="hungFlag" />
    <result column="OP_USER_" jdbcType="VARCHAR" property="opUser" />
    <result column="FORM_KEY_" jdbcType="VARCHAR" property="formKey" />
  </resultMap>
  <sql id="Example_Where_Clause">
    <where>
      <foreach collection="oredCriteria" item="criteria" separator="or">
        <if test="criteria.valid">
          <trim prefix="(" prefixOverrides="and" suffix=")">
            <foreach collection="criteria.criteria" item="criterion">
              <choose>
                <when test="criterion.noValue">
                  and ${criterion.condition}
                </when>
                <when test="criterion.singleValue">
                  and ${criterion.condition} #{criterion.value}
                </when>
                <when test="criterion.betweenValue">
                  and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                </when>
                <when test="criterion.listValue">
                  and ${criterion.condition}
                  <foreach close=")" collection="criterion.value" item="listItem" open="(" separator=",">
                    #{listItem}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>
  <sql id="Base_Column_List">
    PROC_NAME_, STEP_NAME_, CUSTOM_CODE_, TASK_ID_, EXECUTE_ID_, BIZ_ID_, CREATE_TIME_, 
    LOCK_FLAG_, LOCKED_TIME_, ENDLOCK_TIME_, HUNG_FLAG_, OP_USER_, FORM_KEY_
  </sql>
  <select id="selectByExample" parameterType="eastict.sample.pojo.TaskExample" resultMap="BaseResultMap">
    select
    <if test="distinct">
      distinct
    </if>
    <include refid="Base_Column_List" />
    from ru_task
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null">
      order by ${orderByClause}
    </if>
  </select>
  <select id="selectByPrimaryKey" parameterType="eastict.sample.pojo.TaskKey" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from ru_task
    where PROC_NAME_ = #{procName,jdbcType=VARCHAR}
      and STEP_NAME_ = #{stepName,jdbcType=VARCHAR}
      and CUSTOM_CODE_ = #{customCode,jdbcType=VARCHAR}
      and TASK_ID_ = #{taskId,jdbcType=VARCHAR}
  </select>
  <delete id="deleteByPrimaryKey" parameterType="eastict.sample.pojo.TaskKey">
    delete from ru_task
    where PROC_NAME_ = #{procName,jdbcType=VARCHAR}
      and STEP_NAME_ = #{stepName,jdbcType=VARCHAR}
      and CUSTOM_CODE_ = #{customCode,jdbcType=VARCHAR}
      and TASK_ID_ = #{taskId,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" parameterType="eastict.sample.pojo.Task">
    insert into ru_task (PROC_NAME_, STEP_NAME_, CUSTOM_CODE_, 
      TASK_ID_, EXECUTE_ID_, BIZ_ID_, 
      CREATE_TIME_, LOCK_FLAG_, LOCKED_TIME_, 
      ENDLOCK_TIME_, HUNG_FLAG_, OP_USER_, 
      FORM_KEY_)
    values (#{procName,jdbcType=VARCHAR}, #{stepName,jdbcType=VARCHAR}, #{customCode,jdbcType=VARCHAR}, 
      #{taskId,jdbcType=VARCHAR}, #{executeId,jdbcType=VARCHAR}, #{bizId,jdbcType=VARCHAR}, 
      #{createTime,jdbcType=TIMESTAMP}, #{lockFlag,jdbcType=CHAR}, #{lockedTime,jdbcType=TIMESTAMP}, 
      #{endlockTime,jdbcType=TIMESTAMP}, #{hungFlag,jdbcType=CHAR}, #{opUser,jdbcType=VARCHAR}, 
      #{formKey,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="eastict.sample.pojo.Task">
    insert into ru_task
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="procName != null">
        PROC_NAME_,
      </if>
      <if test="stepName != null">
        STEP_NAME_,
      </if>
      <if test="customCode != null">
        CUSTOM_CODE_,
      </if>
      <if test="taskId != null">
        TASK_ID_,
      </if>
