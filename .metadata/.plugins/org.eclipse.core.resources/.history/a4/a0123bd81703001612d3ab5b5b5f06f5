        ENDLOCK_TIME_,
      </if>
      <if test="hungFlag != null">
        HUNG_FLAG_,
      </if>
      <if test="opUser != null">
        OP_USER_,
      </if>
      <if test="formKey != null">
        FORM_KEY_,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="procName != null">
        #{procName,jdbcType=VARCHAR},
      </if>
      <if test="stepName != null">
        #{stepName,jdbcType=VARCHAR},
      </if>
      <if test="customCode != null">
        #{customCode,jdbcType=VARCHAR},
      </if>
      <if test="taskId != null">
        #{taskId,jdbcType=VARCHAR},
      </if>
      <if test="executeId != null">
        #{executeId,jdbcType=VARCHAR},
      </if>
      <if test="bizId != null">
        #{bizId,jdbcType=VARCHAR},
      </if>
      <if test="createTime != null">
        #{createTime,jdbcType=TIMESTAMP},
      </if>
      <if test="lockFlag != null">
        #{lockFlag,jdbcType=CHAR},
      </if>
      <if test="lockedTime != null">
        #{lockedTime,jdbcType=TIMESTAMP},
      </if>
      <if test="endlockTime != null">
        #{endlockTime,jdbcType=TIMESTAMP},
      </if>
      <if test="hungFlag != null">
        #{hungFlag,jdbcType=CHAR},
      </if>
      <if test="opUser != null">
        #{opUser,jdbcType=VARCHAR},
      </if>
      <if test="formKey != null">
        #{formKey,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="eastict.sample.pojo.Task">
    update ru_task
    <set>
      <if test="executeId != null">
        EXECUTE_ID_ = #{executeId,jdbcType=VARCHAR},
      </if>
      <if test="bizId != null">
        BIZ_ID_ = #{bizId,jdbcType=VARCHAR},
      </if>
      <if test="createTime != null">
        CREATE_TIME_ = #{createTime,jdbcType=TIMESTAMP},
      </if>
      <if test="lockFlag != null">
        LOCK_FLAG_ = #{lockFlag,jdbcType=CHAR},
      </if>
      <if test="lockedTime != null">
        LOCKED_TIME_ = #{lockedTime,jdbcType=TIMESTAMP},
      </if>
      <if test="endlockTime != null">
        ENDLOCK_TIME_ = #{endlockTime,jdbcType=TIMESTAMP},
      </if>
      <if test="hungFlag != null">
        HUNG_FLAG_ = #{hungFlag,jdbcType=CHAR},
      </if>
      <if test="opUser != null">
        OP_USER_ = #{opUser,jdbcType=VARCHAR},
      </if>
      <if test="formKey != null">
        FORM_KEY_ = #{formKey,jdbcType=VARCHAR},
      </if>
    </set>
    where PROC_NAME_ = #{procName,jdbcType=VARCHAR}
      and STEP_NAME_ = #{stepName,jdbcType=VARCHAR}
      and CUSTOM_CODE_ = #{customCode,jdbcType=VARCHAR}
      and TASK_ID_ = #{taskId,jdbcType=VARCHAR}
  </update>
