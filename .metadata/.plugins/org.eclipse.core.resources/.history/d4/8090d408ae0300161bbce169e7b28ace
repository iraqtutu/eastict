<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="sample.mapper.TusernodeMapper" >
  <resultMap id="BaseResultMap" type="eastict.pam.pojo.Tusernode" >
    <result column="viewid" property="viewid" jdbcType="BIGINT" />
    <result column="nodeid" property="nodeid" jdbcType="BIGINT" />
    <result column="userid" property="userid" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Example_Where_Clause" >
    <where >
      <foreach collection="oredCriteria" item="criteria" separator="or" >
        <if test="criteria.valid" >
          <trim prefix="(" suffix=")" prefixOverrides="and" >
            <foreach collection="criteria.criteria" item="criterion" >
              <choose >
                <when test="criterion.noValue" >
                  and ${criterion.condition}
                </when>
                <when test="criterion.singleValue" >
                  and ${criterion.condition} #{criterion.value}
                </when>
                <when test="criterion.betweenValue" >
                  and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                </when>
                <when test="criterion.listValue" >
                  and ${criterion.condition}
                  <foreach collection="criterion.value" item="listItem" open="(" close=")" separator="," >
                    #{listItem}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>
  <sql id="Base_Column_List" >
    viewid, nodeid, userid
  </sql>
  <select id="selectByExample" resultMap="BaseResultMap" parameterType="eastict.pam.pojo.TusernodeExample" >
    select
    <if test="distinct" >
      distinct
    </if>
    <include refid="Base_Column_List" />
    from user_node
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null" >
      order by ${orderByClause}
    </if>
  </select>
  <insert id="insert" parameterType="eastict.pam.pojo.Tusernode" >
    insert into user_node (viewid, nodeid, userid
      )
    values (#{viewid,jdbcType=BIGINT}, #{nodeid,jdbcType=BIGINT}, #{userid,jdbcType=VARCHAR}
      )
  </insert>
  <insert id="insertSelective" parameterType="eastict.pam.pojo.Tusernode" >
    insert into user_node
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="viewid != null" >
        viewid,
      </if>
      <if test="nodeid != null" >
        nodeid,
      </if>
      <if test="userid != null" >
        userid,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="viewid != null" >
        #{viewid,jdbcType=BIGINT},
      </if>
      <if test="nodeid != null" >
        #{nodeid,jdbcType=BIGINT},
      </if>
      <if test="userid != null" >
        #{userid,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>